{
  "name": "lrpg",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run build && bun index.ts",
    "dev": "open http://localhost:8800 && nodemon -i 'dist/*' -e ts,css,html -x 'bun start'",
    "build": "rm -rf dist/ && bun build.ts && cp -r spritesheets dist/"
  },
  "devDependencies": {
    "bun-types": "latest",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "matter-js": "^0.19.0",
    "pixi.js": "^7.4.0"
  }
}
